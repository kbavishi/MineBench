package "are two trees identical"
purpose "check if two trees are one"
version "0.1"


option  "tree"			t    "Tree file name ('-' is stdin)" 				string	typestr="FILENAME" default="-" 				no
option  "other"			o    "Other tree file name ('-' is stdin)" 			string	typestr="FILENAME" default="-" 			no
